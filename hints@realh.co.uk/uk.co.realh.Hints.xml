        <method name="FocusedWindowInfo">
            <arg type="i" direction="out" name="x"/>
            <arg type="i" direction="out" name="y"/>
            <arg type="i" direction="out" name="width"/>
            <arg type="i" direction="out" name="height"/>
            <arg type="i" direction="out" name="pid"/>
